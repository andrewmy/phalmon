{
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "ext-gettext": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-phalcon": "~v3.4",
        "dmkit/phalcon-jwt-auth": "^1.0",
        "fzaninotto/faker": "^1.8",
        "mikemirten/json-api": "^0.34.0",
        "phalcon/incubator": "^3.4"
    },
    "require-dev": {
        "codeception/codeception": "^3.1",
        "flow/jsonpath": "^0.4.0",
        "mongodb/mongodb": "^1.4",
        "phalcon/devtools": "^3.4",
        "phalcon/ide-stubs": "^3.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
